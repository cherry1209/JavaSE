public class multiple
{
	public static void main(String[] args)
	{
		int i;
		for(i = 1;i < 10;i++){
			for(int j=1;j <= i;j++){
				int sum;
				sum = i*j;
				System.out.print(j + "*" + i + "=" +sum +" ");
			}
			
			//换行
      //法一：
			System.out.println();
			//法二：
			//System.out.print("\n");
		}
	}
}
